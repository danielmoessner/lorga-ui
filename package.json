  },
  "peerDependencies": {
    "@headlessui/vue": "1.x",
    "@heroicons/vue": "^2.x",
    "vue": "3.x",
    "vue-router": "^4.x"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
