    "nprogress": "^0.2.0",
    "postcss-import": "^15.0.0",
    "rollup-plugin-visualizer": "^5.8.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^5.2.2",
    "vite": "^4.0.3",
    "vitepress": "^1.0.0-alpha.29",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.0.9"
  }
